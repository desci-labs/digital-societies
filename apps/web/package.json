{
  "name": "web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "engines": {
    "node": ">=12.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm lint"
    ]
  },
  "dependencies": {
    "@ethersproject/experimental": "^5.7.0",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.7",
    "@opengsn/provider": "^3.0.0-beta.1",
    "@rainbow-me/rainbowkit": "^0.5.1",
    "@reduxjs/toolkit": "^1.8.5",
    "@svgr/webpack": "^6.5.0",
    "busboy": "^1.6.0",
    "ethers": "^5.0.0",
    "formidable": "^2.0.1",
    "multiformats": "^9.8.1",
    "next": "^12.1.6",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.3.3",
    "react-redux": "^8.0.2",
    "redux-persist": "^6.0.0",
    "wagmi": "^0.6.0",
    "web3.storage": "^4.4.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/formidable": "^2.0.5",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/redux-persist": "^4.3.1",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.2"
  },
  "resolutions": {
    "ethereumjs-abi": "git+https://github.com/ethereumjs/ethereumjs-abi.git"
  }
}
