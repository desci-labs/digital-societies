{
  "name": "web",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm lint"
    ]
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.9.7",
    "@pinata/sdk": "^1.1.26",
    "@rainbow-me/rainbowkit": "^0.5.1",
    "formidable": "^2.0.1",
    "multiformats": "^9.8.1",
    "ethers": "^5.0.0",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "wagmi": "^0.6.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.34.2",
    "react-icons": "^4.4.0",
    "react-loader-spinner": "^5.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/formidable": "^2.0.5",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.2"
  }
}
